{
  "name": "@elias.schaut/webstacklib",
  "version": "1.0.0-alpha.2",
  "description": "My personal lib, when creating full stack web applications. Not recommended to use by others!",
  "homepage": "https://github.com/EliasSchaut/WebStackLib#README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/EliasSchaut/WebStackLib.git"
  },
  "author": "EliasSchaut",
  "private": false,
  "license": "MIT",
  "files": [
    "src",
    "package.json",
    "package-lock.json",
    "tsconfig.build.json",
    "nest-cli.json",
    ".gitignore",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "nest build",
    "build:client": "npm i --prefix ./client && npm run build --prefix ./client",
    "build:db": "npx prisma migrate dev --schema=./prisma/schema.prisma",
    "build:all": "npm run build:client && npm i && npm run build:db && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@nestjs/apollo": "^11.0.5",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/graphql": "^11.0.5",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/serve-static": "^3.0.1",
    "@prisma/client": "^4.13.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cuid": "^3.0.0",
    "graphql": "^16.6.0",
    "helmet": "^6.1.5",
    "joi": "^17.9.2",
    "nestjs-i18n": "^10.2.6",
    "nodemailer": "^6.9.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.3.1",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.1.3"
  }
}
